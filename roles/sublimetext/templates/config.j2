// {{ ansible_managed }}
{
    "draw_white_space": "all",
    "color_scheme": "{{ sublime_text_color_scheme }}",
    "font_options":
    [
        "no_round"
    ],
    "font_size": 11,
    "highlight_line": true,
    "highlight_modified_tabs": true,
    "ignored_packages":
    [
        "Vintage"
    ],
    "scroll_speed": 5.0,
    "tab_size": 4,
    "tabs_small": true,
    "translate_tabs_to_spaces": true,
    "trim_trailing_white_space_on_save": true
}
